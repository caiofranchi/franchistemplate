Schema::table('{{ table }}', function($table)
{
    $table->create();
    $table->increments('id');
    $table->string('name')->unique();
    $table->string('tipo');
    $table->date('nascimento')->nullable();
    $table->text('descricao');
    $table->timestamps(); //Adds created_at and updated_at columns
});