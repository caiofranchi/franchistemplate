{% extends 'admin/master_with_menu.twig' %}

{% block main_content %}

        <div class="page-header">
            <h1 class="">/{{ page_name }} <small>/{{ action }}</small></h1>
        </div>


    <form action="{{ siteUrl }}" class="form-horizontal default-form-validation" role="form" method="post">
                <input type="hidden" name="id" value="{{ table.id }}"/>

        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="nome">nome</label>
                    <div class="col-sm-8">
                                                    <input value="{{ table.nome }}" type="text" class="form-control" id="nome" name="nome" required="required" placeholder="">
                                            </div>
                </div>
            
        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="email">email</label>
                    <div class="col-sm-8">
                                                    <input value="{{ table.email }}" type="email" class="form-control" id="email" name="email" required="required" placeholder="">
                                            </div>
                </div>
            
        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="password">password</label>
                    <div class="col-sm-8">
                                                    <input value="{{ table.password }}" type="text" class="form-control" id="password" name="password" required="required" placeholder="">
                                            </div>
                </div>
            
        
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <!--                            $('#submit-button').button('loading')-->
                <button type="submit" id="submit-button" data-loading-text="Loading..."  class="btn btn-primary">Save</button>
            </div>
        </div>

                                                                                                                                                        
                                                                                                                                                    
                                                                                                        </form>

{% endblock %}