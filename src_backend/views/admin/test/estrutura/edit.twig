{% extends 'admin/master_with_menu.twig' %}

{% block main_content %}

        <div class="page-header">
            <h1 class="">/{{ page_name }} <small>/{{ action }}</small></h1>
        </div>


    <form action="{{ siteUrl }}" class="form-horizontal default-form-validation" role="form" method="post">
                <input type="hidden" name="id" value="{{ table.id }}"/>

        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="nome">nome</label>
                    <div class="col-sm-8">
                                                    <input value="{{ table.nome }}" type="text" class="form-control" id="nome" name="nome" required="required" placeholder="">
                                            </div>
                </div>
            
        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="icone">icone</label>
                    <div class="col-sm-8">
                                                    <input value="{{ table.icone }}" type="text" class="form-control" id="icone" name="icone" required="required" placeholder="">
                                            </div>
                </div>
            
        
                            <div class="form-group">
                    <label class="col-sm-2 control-label" for="descricao">descricao</label>
                    <div class="col-sm-8">
                                                    <textarea class="form-control" rows="3" id="descricao" name="descricao">{{ table.descricao }}</textarea>
                                            </div>
                </div>
            
        

                                                                                                                                                        
                                                                                                                                                    
                                                                                                        </form>

{% endblock %}